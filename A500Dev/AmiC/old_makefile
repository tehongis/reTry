
run		:	main.exe
	fs-uae A500plus.fs-uae


main.exe	:	main.o 
	vlink main.o -o main.exe


#	OPTS=  -Fhunkexe -devpac -nosym -I HDD/srcs/includes -L listfile.txt
main.o	:	main.s
	vasmm68k_mot -Fhunk main.s -o main.o -Iinclude_i -L listfile.txt -DLINUX


main.s	:	main.c
	m68k-linux-gnu-gcc-13 -S -Iinclude_h main.c


clean	:
		rm -rf main.s main.o

