-as=vasmm68k_mot -quiet -Fhunk -nowarn=62 %s -o %s
-asv=vasmm68k_mot -Fhunk -nowarn=62 %s -o %s
-rm=delete quiet %s
-rmv=delete %s
-ld=vlink -bamigahunk -x -Bstatic -Cvbcc -nostdlib -mrel vlibos3:minres.o %s %s -Lvlibos3: -lvcs -o %s
-l2=vlink -bamigahunk -x -Bstatic -Cvbcc -nostdlib -mrel %s %s -Lvlibos3: -o %s
-ldv=vlink -bamigahunk -t -x -Bstatic -Cvbcc -nostdlib -mrel vlibos3:minres.o %s %s -Lvlibos3: -lvcs -o %s
-l2v=vlink -bamigahunk -t -x -Bstatic -Cvbcc -nostdlib -mrel %s %s -Lvlibos3: -o %s
